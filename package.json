    "test:jest": "node node_modules/jest/bin/jest.js",
    "watch:jest": "yarn test:jest -- --watch"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "expo": "^16.0.0",
    "react": "16.0.0-alpha.6",
    "react-native": "^0.43.4"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-mizchi": "^1.0.2",
    "flow-bin": "0.40",
    "jest-expo": "^0.4.0",
    "npm-run-all": "^4.0.2",
    "react-native-scripts": "0.0.29",
    "react-test-renderer": "16.0.0-alpha.6"
  }
}
