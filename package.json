{
  "name": "my-native-app",
  "version": "0.1.0",
  "private": true,
  "main": "./src/native/index.js",
  "scripts": {
    "start": "react-native-scripts start",
    "lint": "eslint src",
    "typecheck": "flow",
    "typecheck:restart": "flow stop; flow",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "run-s lint typecheck test:jest",
    "test:jest": "node node_modules/jest/bin/jest.js",
    "watch:jest": "yarn test:jest -- --watch"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ]
  },
  "dependencies": {
    "expo": "^16.0.0",
    "react": "16.0.0-alpha.6",
    "react-native": "^0.43.4",
    "react-navigation": "^1.0.0-beta.9",
    "react-redux": "^5.0.5",
    "recompose": "^0.23.4",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^2.7.1",
    "eslint": "^3.19.0",
    "eslint-config-mizchi": "^1.0.2",
    "flow-bin": "0.40",
    "jest-expo": "^0.4.0",
    "npm-run-all": "^4.0.2",
    "react-native-scripts": "0.0.29",
    "react-test-renderer": "16.0.0-alpha.6"
  }
}
